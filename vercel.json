{
    "version": 2,
    "builds": [
      {
        "src": "dist/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
     {
        "src": "/assets/(.*)",
        "headers": { "Content-Type": "application/javascript" },
        "dest": "/dist/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "dist/index.html"
      }
    ]
  }
  